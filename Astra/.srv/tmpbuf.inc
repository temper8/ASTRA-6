C----------------------------------------------------------------------|
C NEQNS maximum number of equations:	  NE(1), TE(2), TI(3), CU(4),
C	is defined in 	 		 Equil(5), (6,7,8,9 - free) 
C	for/parameter.inc		  F0, F1, ..., F9 - (10-19)
C The same parameter has to be defined in "for/parameter.inc"
C----------------------------------------------------------------------|
	integer	NTMP, IBUF, IVBUF
	parameter (NTMP=180)
	integer	LINAPP(NEQNS),  NBEG(NTMP), LTMPBUF(NTMP),
     &	LCC,  LCD,  LCE,  LCI,  LCN,  LCU,  LCV,  LDC,  LDE,  LDI,  ! 1-10
     1  LDN,  LHC,  LHE,  LHI,  LHN,  LIPL, LLEXT,LMU,  LMV,  LNE,  !11-20
     2  LNEB, LPE,  LPET, LPI,  LPIT, LCUBS,                        !21-26
     3	LQNB, LQNNB,LQEB, LQETB,LQIB, LQITB,LROE, LROI, LRON,       !27-35
     4  LSN,  LSNN, LTE,  LTEB, LTI,  LTIB, LUEXT,                  !36-42
     5	LXC,  LXE,  LXI,  LXN,  LDVN, LDSN, LDVE, LDSE, LDVI, LDSI, !43-52
     5	LNI,  LDUM2,LDUM3,LDUM4,LDUM5,LDUM6,LDUM7,LDUM8             !53-60
	integer
     6	LRO0, LRO1, LRO2, LRO3, LRO4, LRO5, LRO6, LRO7, LRO8, LRO9, !61-70
     7	LF0,  LF1,  LF2,  LF3,  LF4,  LF5,  LF6,  LF7,  LF8,  LF9,  !71-80
     8  LDF0, LDF1, LDF2, LDF3, LDF4, LDF5, LDF6, LDF7, LDF8, LDF9, !81-90
     9	LVF0, LVF1, LVF2, LVF3, LVF4, LVF5, LVF6, LVF7, LVF8, LVF9, !91-100
     &	LGF0, LGF1, LGF2, LGF3, LGF4, LGF5, LGF6, LGF7, LGF8, LGF9, !101-110
     1	LSF0, LSF1, LSF2, LSF3, LSF4, LSF5, LSF6, LSF7, LSF8, LSF9, !111-120
     2	LSFF0,LSFF1,LSFF2,LSFF3,LSFF4,LSFF5,LSFF6,LSFF7,LSFF8,LSFF9,!121-130
     3	LF0B, LF1B, LF2B, LF3B, LF4B, LF5B, LF6B, LF7B, LF8B, LF9B, !131-140
     4	LQF0B,LQF1B,LQF2B,LQF3B,LQF4B,LQF5B,LQF6B,LQF7B,LQF8B,LQF9B,!141-150
     5  LQFF0B,LQFF1B,LQFF2B,LQFF3B,LQFF4B,LQFF5B,LQFF6B,LQFF7B,    !151-158
     5	LQFF8B,LQFF9B,                                              !159-160
     6	LDVF0,LDVF1,LDVF2,LDVF3,LDVF4,LDVF5,LDVF6,LDVF7,LDVF8,LDVF9,!161-170
     7	LDSF0,LDSF1,LDSF2,LDSF3,LDSF4,LDSF5,LDSF6,LDSF7,LDSF8,LDSF9 !171-180
	integer	LF(0:9), LDF(0:9), LVF(0:9), LGF(0:9), LSF(0:9),
     1		LRO(0:9),LFB(0:9), LQFB(0:9),LSFF(0:9),LQFFB(0:9),
     2		LDVF(0:9),LDSF(0:9)
	equivalence     (LF(0),LF0), (LQFB(0),LQF0B), (LQFFB(0),LQFF0B)
	equivalence     (LDF(0),LDF0),  (LVF(0),LVF0),  (LGF(0),LGF0)
	equivalence     (LSF(0),LSF0),  (LFB(0),LF0B),  (LRO(0),LRO0)
	equivalence     (LSFF(0),LSFF0),(LDVF(0),LDVF0),(LDSF(0),LDSF0)
	common  /TMPFIL/  NBEG, IBUF, IVBUF,
C Altogether NTMP=180: LNE -> LDSF9
     &	LNE,  LDN,  LHN,  LXN,  LCN,  LSN,  LSNN, LNEB, LQNB, LQNNB,   !  1-10
     1	LTE,  LDE,  LHE,  LXE,  LCE,  LPE,  LPET, LTEB, LQEB, LQETB,   ! 11-20
     2	LTI,  LDI,  LHI,  LXI,  LCI,  LPI,  LPIT, LTIB, LQIB, LQITB,   ! 21-30
     3	LCU,  LDC,  LHC,  LXC,  LCV,  LCD,  LCC,  LIPL, LLEXT,LUEXT,   ! 31-40
     4	LMU,  LMV,  LCUBS,LRON, LROE, LROI, LDVN, LDSN, LDVE, LDSE,    ! 41-50
     5	LDVI, LDSI, LNI,  LDUM2,LDUM3,LDUM4,LDUM5,LDUM6,LDUM7,LDUM8,   ! 51-60
     6	LRO0, LRO1, LRO2, LRO3, LRO4, LRO5, LRO6, LRO7, LRO8, LRO9,    ! 61-70
     7	LF0,  LF1,  LF2,  LF3,  LF4,  LF5,  LF6,  LF7,  LF8,  LF9,     ! 71-80
     8  LDF0, LDF1, LDF2, LDF3, LDF4, LDF5, LDF6, LDF7, LDF8, LDF9,    ! 81-90
     9	LVF0, LVF1, LVF2, LVF3, LVF4, LVF5, LVF6, LVF7, LVF8, LVF9,    ! 91-100
     &	LSF0, LSF1, LSF2, LSF3, LSF4, LSF5, LSF6, LSF7, LSF8, LSF9,    !101-110
     1	LSFF0,LSFF1,LSFF2,LSFF3,LSFF4,LSFF5,LSFF6,LSFF7,LSFF8,LSFF9,   !111-120
     2	LGF0, LGF1, LGF2, LGF3, LGF4, LGF5, LGF6, LGF7, LGF8, LGF9,    !121-130
     3	LF0B, LF1B, LF2B, LF3B, LF4B, LF5B, LF6B, LF7B, LF8B, LF9B,    !131-140
     4	LQF0B,LQF1B,LQF2B,LQF3B,LQF4B,LQF5B,LQF6B,LQF7B,LQF8B,LQF9B,   !141-150
     5  LQFF0B,LQFF1B,LQFF2B,LQFF3B,LQFF4B,LQFF5B,LQFF6B,LQFF7B,LQFF8B,!151-160
     5	LQFF9B,
     6	LDVF0,LDVF1,LDVF2,LDVF3,LDVF4,LDVF5,LDVF6,LDVF7,LDVF8,LDVF9,   !161-170
     7	LDSF0,LDSF1,LDSF2,LDSF3,LDSF4,LDSF5,LDSF6,LDSF7,LDSF8,LDSF9    !171-180
	equivalence     (LTMPBUF(1),LNE)
	character 	  TMPBUF*1,	DTVBUF*1,    TMPNAM*6
	common	/TMPBUF/  LINAPP,TMPBUF(10000),DTVBUF(5000),TMPNAM(NTMP)
C----------------------------------------------------------------------|
